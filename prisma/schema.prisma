generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Note {
  id      String @id @default(uuid())
  subject String
  content String
  updatedAt DateTime @updatedAt
  createdAt DateTime @default(now())
}
